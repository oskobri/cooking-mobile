{
  "name": "cooking-website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "sync": "npm run build && npx cap sync",
    "ios": "npm run build && npx cap sync && npx cap open ios",
    "dev-copy": "NODE_ENV=development npx cap copy",
    "dev-ios": "NODE_ENV=development npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/status-bar": "^6.0.1",
    "@vueuse/components": "^11.1.0",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
  }
